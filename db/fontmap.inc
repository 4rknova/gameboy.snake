; ----------------------------------------------------
; ASCII 'A'..'Z' -> tile ID lookup table (DMG BG font)
; ----------------------------------------------------

FontMapAtoZ:      ; base label for A..Z mapping
    db TILE_A     ; A -> tile 'A'
    db TILE_EMPTY ; B -> unused -> empty
    db TILE_EMPTY ; C -> unused -> empty
    db TILE_EMPTY ; D -> unused -> empty
    db TILE_E     ; E -> tile 'E'
    db TILE_EMPTY ; F -> unused -> empty
    db TILE_G     ; G -> tile 'G'
    db TILE_EMPTY ; H -> unused -> empty
    db TILE_EMPTY ; I -> unused -> empty
    db TILE_EMPTY ; J -> unused -> empty
    db TILE_K     ; K -> tile 'K'
    db TILE_EMPTY ; L -> unused -> empty
    db TILE_M     ; M -> tile 'M'
    db TILE_N     ; N -> tile 'N'
    db TILE_O     ; O -> tile 'O'
    db TILE_P     ; P -> tile 'P'
    db TILE_EMPTY ; Q -> unused -> empty
    db TILE_R     ; R -> tile 'R'
    db TILE_S     ; S -> tile 'S'
    db TILE_T     ; T -> tile 'T'
    db TILE_EMPTY ; U -> unused -> empty
    db TILE_V     ; V -> tile 'V'
    db TILE_EMPTY ; W -> unused -> empty
    db TILE_EMPTY ; X -> unused -> empty
    db TILE_EMPTY ; Y -> unused -> empty
    db TILE_EMPTY ; Z -> unused -> empty